{
  "name": "sens-eye",
  "version": "1.1.6",
  "description": "A library for detecting sensitive info in images using Tesseract.js",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --config jest.config.cjs",
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/sharp": "^0.32.0",
    "sharp": "^0.32.6",
    "tesseract.js": "^6.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current",
        "linux"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "ignoredBuiltDependencies": [
      "esbuild",
      "sharp",
      "tesseract.js"
    ],
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "tesseract.js"
    ]
  }
}
